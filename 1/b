#!/usr/bin/env raku

sub MAIN(
    Str $infile where *.IO.f = 'input'
) {
    my @lines = $infile.IO.lines;
    @lines = @lines.map({$_.Int}).sort;

    my $n = @lines.combinations(2)
        .map({ ($_[0] + $_[1], $_[0] * $_[1]) })
        .first({ (2020 - $_[0]) âˆˆ @lines });
        
    say $n[1] * (2020 - $n[0]);
}